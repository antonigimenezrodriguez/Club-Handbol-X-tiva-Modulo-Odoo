<?xml version="1.0"?>
<odoo>
    <record model="ir.actions.act_window" id="action_xativa_esponsor">
        <field name="name">Esponsor</field>
        <field name="res_model">xativa.esponsor</field>
        <field name="view_mode">tree,form</field>
    </record>

	<menuitem name="Esponsors" parent="xativa_menu_economica" id="xativa_menu_2_esponsor" action="action_xativa_esponsor" />

    <record model="ir.ui.view" id="view_xativa_esponsor_tree">
        <field name="name">view_xativa_esponsor_tree</field>
        <field name="model">xativa.esponsor</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree string="esponsor">
                <field name="numero" />
                <field name="c" />
                <field name="nomEsponsor" />
                <field name="empresa_id" />
                <field name="importSenseIVA" widget="monetary"/>
                <field name="importAmbIVA" widget="monetary"/>
                <field name="equips_ids" />
                <field name="temporada_id" />
                <field name="cobrat" />
                <field name="persona_id" />
                <field name="importDesgravat" />
            </tree>
        </field>
    </record>

</odoo>